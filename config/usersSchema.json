{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "http://example.com/example.json",
  "title": "User Schema",
  "description": "Information of a user",
  "first_name": {
    "description": "FirstName",
    "type": "string",
    "minLength": 1
  },
  "last_name": {
    "description": "LastName",
    "type": "string",
    "minLength": 1
  },
  "nickname": {
    "description": "Nickname",
    "type": "string",
    "minLength": 1
  },
  "password": {
    "description": "Password",
    "type": "string",
    "minLength": 1
  },
  "email": {
    "description": "Email",
    "type": "string",
    "minLength": 1
  },
  "country": {
    "description": "Country",
    "type": "string",
    "minLength": 1
  },
  "required": ["first_name", "nickname", "password", "email", "country"]
}
